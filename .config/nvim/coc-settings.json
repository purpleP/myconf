{
  "languageserver": {
    "hie": {
      "command": "hie-wrapper",
      "filetypes": ["haskell"],
      "rootPatterns": ["package.yaml"]
    },
    "clangd": {
      "command": "clangd",
      "filetypes": ["c", "cpp", "objc", "objcpp"]
    }
  },
  "java.signatureHelp.enabled": true,
  "python.jediEnabled": false
}
